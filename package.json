{
  "name": "yapi-vendor",
  "version": "1.9.2",
  "description": "YAPI",
  "main": "server/app.js",
  "scripts": {
    "postinstall": "concurrently \"cd server && npm i\" \"cd client && npm i\"",
    "clean": "rimraf ./node_modules/ ./server/node_modules/ ./client/node_modules/",
    "dev": "concurrently -k \"cd server && npm run dev\" \"cd client && npm run dev\"",
    "install-server": "node server/install.js",
    "start": "node server/app.js",
    "test": "ava",
    "npm-publish": "node ./npm-publish.js",
    "docs": "ydoc build"
  },
  "scripts-info": {
    "dev": "运行开发服务器",
    "start": "运行生产环境服务器",
    "install-server": "初始化数据库数据，用于安装",
    "test": "执行测试"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:YApi-Group/yapi.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "ajv": "^5.5.1",
    "ajv-i18n": "^2.2.0",
    "axios": "0.18.1",
    "buffer": "^6.0.3",
    "compare-versions": "^3.4.0",
    "copy-to-clipboard": "3.0.8",
    "cpu-load": "^1.0.0",
    "crypto-js": "^3.1.9-1",
    "easy-json-schema": "0.0.2-beta",
    "fs-extra": "^3.0.1",
    "generate-schema": "^2.6.0",
    "js-base64": "^3.6.1",
    "json-schema-faker": "^0.5.0-rc16",
    "json-schema-ref-parser": "4.0.0",
    "json5": "0.5.1",
    "jsondiffpatch": "0.3.11",
    "jsrsasign": "^8.0.12",
    "markdown-it": "12.0.6",
    "markdown-it-anchor": "7.1.0",
    "markdown-it-include": "^2.0.0",
    "markdown-it-table-of-contents": "0.5.2",
    "md5": "2.2.1",
    "mockjs": "1.0.1-beta3",
    "moment": "^2.19.3",
    "moox": "^1.0.2",
    "node-schedule": "^1.3.2",
    "nodemailer": "4.0.1",
    "qs": "^6.7.0",
    "request": "2.81.0",
    "sha.js": "2.4.11",
    "sha1": "1.1.1",
    "swagger-client": "^3.5.1",
    "underscore": "1.8.3",
    "url": "0.11.0",
    "vm-browserify": "^1.1.2",
    "vm2": "^3.8.4",
    "yapi-plugin-qsso": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/eslint-parser": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/plugin-transform-typescript": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "antd": "4.16.2",
    "anujs": "^1.2.6",
    "ava": "^0.22.0",
    "brace": "^0.10.0",
    "buffer-shims": "^1.0.0",
    "concurrently": "^6.2.1",
    "core-decorators": "^0.17.0",
    "eslint": "^7.28.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "ghooks": "^2.0.0",
    "nodemon": "^2.0.12",
    "prop-types": "^15.5.10",
    "rewire": "^2.5.2",
    "typescript": "^4.3.4",
    "validate-commit-msg": "^2.12.2"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "test",
        "chore",
        "refactor",
        "opti"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "请输入message信息!",
      "helpMessage": "Commit message 格式错误， http://www.ruanyifeng.com/blog/2016/01/commit_message_change_log.html"
    }
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
